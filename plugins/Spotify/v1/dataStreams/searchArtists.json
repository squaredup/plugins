{
    "name": "searchArtists",
    "config": {
        "httpMethod": "get",
        "endpointPath": "/search",
        "getArgs": [
            {
                "value": "artist",
                "key": "type"
            },
            {
                "value": "{{q}}",
                "key": "q"
            }
        ],
        "pathToData": "artists.items",
        "headers": [],
        "expandInnerObjects": true
    },
    "metadata": [
        {
            "shape": [
                "number",
                {
                    "decimalPlaces": 0
                }
            ],
            "name": "followers.total",
            "displayName": "Followers"
        },
        {
            "shape": [
                "number",
                {
                    "decimalPlaces": 0
                }
            ],
            "name": "popularity"
        },
        {
            "pattern": ".*"
        }
    ],
    "timeframes": false,
    "displayName": "Artists",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none",
    "ui": [
        {
            "name": "q",
            "label": "Search Query",
            "title": "Search Query",
            "type": "text"
        }
    ]
}
{
    "name": "getPlaylists",
    "config": {
        "httpMethod": "get",
        "expandInnerObjects": true,
        "endpointPath": "/me/playlists",
        "getArgs": [
            {
                "key": "limit",
                "value": "50"
            }
        ],
        "pathToData": "items",
        "headers": []
    },
    "metadata": [
        {
            "displayName": "Track Count",
            "shape": [
                "number",
                {
                    "decimalPlaces": 0
                }
            ],
            "name": "tracks.total"
        },
        {
            "pattern": ".*"
        }
    ],
    "timeframes": false,
    "displayName": "Playlists",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none"
}
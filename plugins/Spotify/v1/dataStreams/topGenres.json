{
    "name": "topGenres",
    "config": {
        "httpMethod": "get",
        "endpointPath": "/me/top/artists",
        "getArgs": [
            {
                "value": "{{time_range}}",
                "key": "time_range"
            },
            {
                "value": "50",
                "key": "limit"
            },
            {
                "key": "type",
                "value": "artists"
            }
        ],
        "pathToData": "items",
        "headers": [],
        "expandInnerObjects": true
    },
    "metadata": [
        {
            "pattern": ".*"
        }
    ],
    "timeframes": false,
    "defaultShaping": {
        "group": {
            "by": [
                [
                    "genres.0",
                    "uniqueValues"
                ]
            ],
            "aggregate": [
                {
                    "type": "count"
                }
            ]
        }
    },
    "displayName": "Top Genres",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none",
    "ui": [
        {
            "isMulti": false,
            "data": {
                "source": "fixed",
                "values": [
                    {
                        "value": "short_term",
                        "label": "Short Term (Past 4 Weeks)"
                    },
                    {
                        "value": "medium_term",
                        "label": "Medium Term (Past 6 Months)"
                    },
                    {
                        "value": "long_term",
                        "label": "Long Term (Past Year)"
                    }
                ]
            },
            "name": "time_range",
            "label": "Time Range",
            "title": "Time Range",
            "type": "autocomplete",
            "validation": {
                "required": true
            }
        }
    ]
}
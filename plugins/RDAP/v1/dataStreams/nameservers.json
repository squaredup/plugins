{
    "name": "nameservers",
    "config": {
        "httpMethod": "get",
        "expandInnerObjects": true,
        "endpointPath": "domain/{{domain}}",
        "headers": [
            {
                "key": "accept",
                "value": "*/*"
            }
        ],
        "postRequestScript": "nameservers.js"
    },
    "timeframes": false,
    "displayName": "Nameservers",
    "description": "The list of nameservers registered for the domain",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none",
    "ui": [
        {
            "name": "domain",
            "label": "Domain (top-level domain name)",
            "placeholder": "mydomain.com",
            "type": "text",
            "validation": {
                "required": true
            }
        }
    ]
}
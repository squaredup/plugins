{
    "config": {
        "endpointPath": "/ea/isp-metrics/5m",
        "expandInnerObjects": true,
        "getArgs": [
            {
                "key": "beginTimestamp",
                "value": "{{timeframe.start}}"
            }
        ],
        "headers": [],
        "httpMethod": "get",
        "pagingConfig_mode": "none",
        "pagingConfig_offset_base": 1,
        "postRequestScript": "wanMetrics.js"
    },
    "baseDataSourceName": "httpRequestUnscoped",
    "displayName": "WAN Metrics",
    "manualConfigApply": true,
    "matches": "none",
    "metadata": [
        {
            "name": "avgLatency",
            "shape": [
                "milliseconds",
                {
                    "decimalPlaces": 0,
                    "formatDuration": false
                }
            ]
        },
        {
            "name": "download_kbps",
            "displayName": "Download Speed",
            "formatExpression": "{{ $['download_kbps'] / 1000 }} Mbps",
            "shape": "number"
        },
        {
            "name": "maxLatency",
            "shape": [
                "milliseconds",
                {
                    "decimalPlaces": 0,
                    "formatDuration": false
                }
            ]
        },
        {
            "name": "upload_kbps",
            "displayName": "Upload Speed",
            "formatExpression": "{{ $['upload_kbps'] / 1000 }} Mbps",
            "shape": "number"
        },
        {
            "name": "uptime",
            "shape": [
                "percent",
                {
                    "asZeroToOne": false,
                    "decimalPlaces": 0
                }
            ]
        },
        {
            "pattern": ".*"
        }
    ],
    "name": "wanMetrics",
    "rowPath": [],
    "timeframes": ["last1hour", "last12hours", "last24hours"]
}

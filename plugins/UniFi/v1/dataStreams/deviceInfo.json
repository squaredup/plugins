{
    "config": {
        "endpointPath": "/v1/devices",
        "expandInnerObjects": true,
        "getArgs": [],
        "headers": [],
        "httpMethod": "get",
        "postRequestScript": "deviceInfo.js"
    },
    "baseDataSourceName": "httpRequestScoped",
    "displayName": "Device Info",
    "matches": {
        "sourceType": {
            "type": "oneOf",
            "values": ["UniFi Protect", "UniFi Network", "UniFi Access"]
        }
    },
    "metadata": [
        {
            "name": "name",
            "role": "label"
        },
        {
            "name": "id",
            "role": "id"
        },
        {
            "name": "sourceId",
            "shape": "string",
            "visible": false
        },
        {
            "name": "status",
            "shape": "string"
        },
        {
            "name": "state",
            "shape": [
                "state",
                {
                    "map": {
                        "error": ["offline"],
                        "success": ["online"],
                        "unknown": [],
                        "unmonitored": [],
                        "warning": []
                    }
                }
            ]
        },
        {
            "pattern": ".*"
        }
    ],
    "name": "deviceInfo",
    "timeframes": false
}

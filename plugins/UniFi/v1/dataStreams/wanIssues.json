{
    "config": {
        "endpointPath": "/v1/sites",
        "expandInnerObjects": true,
        "getArgs": [],
        "headers": [],
        "httpMethod": "get",
        "pagingConfig_mode": "none",
        "pagingConfig_offset_base": 1,
        "postRequestScript": "wanIssues.js"
    },
    "baseDataSourceName": "httpRequestUnscoped",
    "defaultShaping": {
        "sort": {
            "by": [["index", "desc"]]
        }
    },
    "displayName": "WAN Issues",
    "manualConfigApply": true,
    "matches": "none",
    "metadata": [
        {
            "displayName": "Alert ID",
            "name": "index",
            "shape": "string"
        },
        {
            "name": "latencyMaxMs",
            "shape": [
                "milliseconds",
                {
                    "decimalPlaces": 0,
                    "formatDuration": false
                }
            ]
        },
        {
            "name": "latencyAvgMs",
            "shape": [
                "milliseconds",
                {
                    "decimalPlaces": 0,
                    "formatDuration": false
                }
            ]
        },
        {
            "name": "count",
            "shape": [
                "number",
                {
                    "decimalPlaces": 0
                }
            ]
        },
        {
            "pattern": ".*"
        }
    ],
    "name": "wanIssues",
    "rowPath": [],
    "timeframes": false
}

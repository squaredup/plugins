{
    "name": "phare",
    "displayName": "Phare",
    "version": "1.0.2",
    "author": {
        "name": "@vinbab",
        "type": "community"
    },
    "description": "Monitor the health and performance of your Phare monitors.",
    "category": "Monitoring",
    "type": "cloud",
    "restrictedToPlatforms": [],
    "importNotSupported": true,
    "schemaVersion": "2.0",
    "base": {
        "plugin": "WebAPI",
        "majorVersion": "1",
        "config": {
            "headers": [
                {
                    "value": "Bearer {{apiKey}}",
                    "key": "Authorization"
                }
            ],
            "authMode": "none",
            "baseUrl": "https://api.phare.io",
            "ignoreCertificateErrors": false
        }
    },
    "links": [
        {
            "category": "source",
            "url": "https://github.com/squaredup/plugins/tree/main/plugins/Phare/v1",
            "label": "Repository"
        }
    ]
}

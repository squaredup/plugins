{
    "timeframes": false,
    "name": "incidents",
    "config": {
        "httpMethod": "get",
        "endpointPath": "/uptime/incidents",
        "pathToData": "data",
        "getArgs": [],
        "headers": []
    },
    "manualConfigApply": true,
    "rowPath": [],
    "metadata": [
        {
            "formatExpression": "{{ ({\n  'maintenance': 'Maintenance',\n  'degradedPerformance': 'Degraded Performance',\n  'unknown': 'Unknown',\n  'partialOutage': 'Partial Outage',\n  'majorOutage': 'Major Outage'\n})[$['impact']] || $['impact'] }}\n",
            "name": "impact"
        },
        {
            "shape": "string",
            "name": "id"
        },
        {
            "shape": "string",
            "name": "project_id"
        },
        {
            "shape": [
                "state",
                {
                    "map": {
                        "unmonitored": [],
                        "unknown": [],
                        "error": [
                            "ongoing"
                        ],
                        "warning": [],
                        "success": [
                            "recovered"
                        ]
                    }
                }
            ],
            "name": "status"
        },
        {
            "formatExpression": "{{ ({\n  'investigating': 'Investigating',\n  'unknown': 'Unknown'\n})[$['state']] || $['state'] }}",
            "name": "state"
        },
        {
            "pattern": ".*"
        }
    ],
    "displayName": "Incidents",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none"
}
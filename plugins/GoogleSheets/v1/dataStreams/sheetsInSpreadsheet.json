            }
        ],
        "postRequestScript": "sheetsInSpreadsheet.js",
        "headers": []
    },
    "matches": "none",
    "visibility": {
        "type": "hidden"
    },
    "metadata": [
        {
            "name": "label",
            "role": "label"
        },
        {
            "name": "value",
            "role": "value"
        }
    ],
    "timeframes": false
}

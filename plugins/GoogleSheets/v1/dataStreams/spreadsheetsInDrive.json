{
    "name": "spreadsheetsInDrive",
    "displayName": "Spreadsheets in Drive",
    "baseDataSourceName": "httpRequestUnscoped",
    "config": {
        "httpMethod": "get",
        "baseUrl": "https://www.googleapis.com/drive/v3",
        "endpointPath": "/files",
        "getArgs": [
            {
                "key": "q",
                "value": "mimeType='application/vnd.google-apps.spreadsheet'"
            },
            {
                "key": "fields",
                "value": "nextPageToken, files(id, name)"
            }
        ],
        "postRequestScript": "spreadsheetsInDrive.js",
        "headers": []
    },
    "visibility": {
        "type": "hidden"
    },
    "metadata": [
        {
            "name": "label",
            "role": "label"
        },
        {
            "name": "value",
            "role": "value"
        }
    ],
    "timeframes": false
}
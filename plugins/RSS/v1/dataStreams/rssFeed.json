{
    "name": "rssFeed",
    "displayName": "RSS Feed",
    "baseDataSourceName": "httpRequestUnscoped",
    "config": {
        "httpMethod": "get",
        "paging": {
            "mode": "none"
        },
        "expandInnerObjects": true,
        "pathToData": "{{typeof pathToData !== 'undefined' ? pathToData : undefined}}",
        "getArgs": [],
        "headers": [],
        "postRequestScript": "{{typeof postRequestScript !== 'undefined' ? postRequestScript : undefined}}"
    },
    "timeframes": false,
    "ui": [
        {
            "type": "markdown",
            "name": "help",
            "content": "#### Usage\n\nThis data stream displays items from a **simple** RSS feed. If you need more control over the request, use the **Web API** data source and enter the RSS feed URL as the Base URL.\n\n---"
        },
        {
            "help": "**Path**: specify a key/path from the returned data to select.\n\n**Script**: provide a JavaScript function to post-process the data",
            "visible": "true",
            "displayAs": "fieldGroupToggle",
            "name": "response",
            "label": "Select data",
            "type": "fieldGroup",
            "fields": [
                {
                    "defaultValue": "rss.channel[0].item",
                    "name": "pathToData",
                    "label": "Path",
                    "placeholder": "rss.channel[0].item",
                    "title": "Path to data",
                    "type": "text",
                    "validation": {
                        "required": false
                    }
                },
                {
                    "name": "postRequestScript",
                    "help": "Executes a script on the raw data, allowing you to preprocess or restructure the data as needed",
                    "label": "Script",
                    "placeholder": "result = data;",
                    "type": "script"
                }
            ]
        }
    ]
}
{
    "name": "incidents",
    "config": {
        "httpMethod": "get",
        "expandInnerObjects": true,
        "endpointPath": "incidents",
        "getArgs": [
            {
                "key": "filter[{{timestampField}}][gte]",
                "value": "{{{timeframe.start}}}"
            },
            {
                "key": "filter[{{timestampField}}][lte]",
                "value": "{{{timeframe.end}}}"
            },
            {
                "key": "page[number]",
                "value": "1"
            },
            {
                "key": "page[size]",
                "value": "100"
            }
        ],
        "postRequestScript": "incidents.js",
        "headers": []
    },
    "manualConfigApply": true,
    "rowPath": [],
    "metadata": [
        {
            "shape": "string",
            "name": "id"
        },
        {
            "pattern": ".*"
        }
    ],
    "timeframes": [
        "last1hour",
        "last12hours",
        "last24hours",
        "last7days",
        "last30days",
        "thisMonth",
        "thisQuarter",
        "thisYear",
        "lastMonth",
        "lastQuarter",
        "lastYear"
    ],
    "displayName": "Incidents",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none",
    "ui": [
        {
            "help": "Select which timestamp field Rootly should filter by.",
            "defaultValue": "created_at",
            "name": "timestampField",
            "options": [
                {
                    "value": "created_at",
                    "label": "Created"
                },
                {
                    "value": "updated_at",
                    "label": "Updated"
                },
                {
                    "value": "started_at",
                    "label": "Started"
                },
                {
                    "value": "detected_at",
                    "label": "Detected"
                },
                {
                    "value": "acknowledged_at",
                    "label": "Acknowledged"
                },
                {
                    "value": "mitigated_at",
                    "label": "Mitigated"
                },
                {
                    "value": "resolved_at",
                    "label": "Resolved"
                },
                {
                    "value": "closed_at",
                    "label": "Closed"
                }
            ],
            "label": "Timestamp Field",
            "title": "Timestamp Field",
            "type": "radio",
            "validation": {
                "required": true
            }
        }
    ]
}
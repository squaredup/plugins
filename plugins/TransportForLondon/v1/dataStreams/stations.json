{
    "name": "stations",
    "displayName": "Stations",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none",
    "config": {
        "httpMethod": "get",
        "expandInnerObjects": true,
        "endpointPath": "StopPoint/Mode/tube",
        "getArgs": [],
        "postRequestScript": "stations.js",
        "headers": []
    },
    "supportsNoneTimeframe": true,
    "defaultTimeframe": "none",
    "manualConfigApply": true,
    "rowPath": [],
    "metadata": [
        {
            "shape": [
                "number",
                {
                    "decimalPlaces": 6
                }
            ],
            "name": "lat"
        },
        {
            "shape": [
                "number",
                {
                    "decimalPlaces": 6
                }
            ],
            "name": "lon"
        },
        {
            "pattern": ".*"
        }
    ],
    "defaultShaping": {
        "sort": {
            "by": [
                [
                    "name",
                    "asc"
                ]
            ]
        }
    }
}
{
    "name": "arrivals",
    "displayName": "Arrivals",
    "baseDataSourceName": "httpRequestScoped",
    "matches": {
        "sourceType": {
            "type": "equals",
            "value": "station"
        }
    },
    "config": {
        "httpMethod": "get",
        "endpointPath": "/StopPoint/{{variable1[0].stationId}}/Arrivals",
        "getArgs": [],
        "headers": [],
        "postRequestScript": "arrivals.js"
    },
    "supportsNoneTimeframe": true,
    "defaultTimeframe": "none",
    "manualConfigApply": true,
    "rowPath": [],
    "metadata": [
        {
            "shape": [
                "seconds",
                {
                    "decimalPlaces": 0,
                    "formatDuration": true
                }
            ],
            "name": "timeToStation"
        },
        {
            "pattern": ".*"
        }
    ]
}
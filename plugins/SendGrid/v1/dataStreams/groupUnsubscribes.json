{
    "name": "groupUnsubscribes",
    "displayName": "Group Unsubscribes",
    "baseDataSourceName": "httpRequestUnscoped",
    "config": {
        "httpMethod": "get",
        "paging": {
            "mode": "none"
        },
        "expandInnerObjects": true,
        "endpointPath": "/asm/suppressions",
        "getArgs": [
            {
                "key": "start_time",
                "value": "{{timeframe.unixStart}}"
            },
            {
                "key": "end_time",
                "value": "{{timeframe.unixEnd}}"
            }
        ],
        "pathToData": "",
        "headers": []
    },
    "metadata": [
        {
            "shape": [
                "date",
                {
                    "timeZone": "Etc/UTC"
                }
            ],
            "name": "created_at"
        },
        {
          "name": "email",
          "shape": "string"
        },
        {
          "name": "group_name",
          "shape": "string"
        },
        {
          "name": "group_id",
          "shape": "string"
        },
        {
            "pattern": ".*"
        }
    ],
    "timeframes": true
}
{
    "name": "bounces",
    "displayName": "Bounces",
    "baseDataSourceName": "httpRequestUnscoped",
    "config": {
        "httpMethod": "get",
        "paging": {
            "mode": "none"
        },
        "expandInnerObjects": true,
        "endpointPath": "/suppression/bounces",
        "getArgs": [
            {
                "key": "start_time",
                "value": "{{timeframe.unixStart}}"
            },
            {
                "key": "end_time",
                "value": "{{timeframe.unixEnd}}"
            },
            {
                "key": "limit",
                "value": "500"
            }
        ],
        "pathToData": "",
        "headers": []
    },
    "metadata": [
        {
            "shape": [
                "date",
                {
                    "timeZone": "Etc/UTC"
                }
            ],
            "name": "created"
        },
        {
            "pattern": ".*"
        }
    ],
    "timeframes": true
}
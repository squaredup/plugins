{
    "name": "HourlyForecast",
    "config": {
        "httpMethod": "get",
        "endpointPath": "point/hourly",
        "pathToData": "features[0].properties.timeSeries",
        "getArgs": [
            {
                "key": "longitude",
                "value": "{{longitude}}",
                "needsEncryption": "false"
            },
            {
                "key": "latitude",
                "value": "{{latitude}}",
                "needsEncryption": "false"
            }
        ],
        "headers": [],
        "expandInnerObjects": true
    },
    "manualConfigApply": true,
    "rowPath": [],
    "metadata": [],
    "timeframes": [
        "last1hour",
        "last12hours",
        "last24hours",
        "last7days",
        "last30days",
        "thisMonth",
        "thisQuarter",
        "thisYear",
        "lastMonth",
        "lastQuarter",
        "lastYear",
        "none"
    ],
    "displayName": "Hourly forecast",
    "description": "Find nearest forecast data that matches the request criteria",
    "baseDataSourceName": "httpRequestUnscoped",
    "matches": "none",
    "ui": [
        {
            "name": "longitude",
            "label": "Longitude",
            "title": "Longitude",
            "type": "number",
            "validation": {
                "required": true,
                "min": -180,
                "max": 180
            }
        },
        {
            "name": "latitude",
            "label": "Latitude",
            "title": "Latitude",
            "type": "number",
            "validation": {
                "required": true,
                "min": -85,
                "max": 85
            }
        }
    ]
}
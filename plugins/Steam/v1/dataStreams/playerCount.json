{
    "name": "playerCount",
    "displayName": "Player Count",
    "baseDataSourceName": "httpRequestUnscoped",
    "config": {
        "httpMethod": "get",
        "paging": {
            "mode": "none"
        },
        "expandInnerObjects": true,
        "endpointPath": "/ISteamUserStats/GetNumberOfCurrentPlayers/v1/",
        "getArgs": [
            {
                "key": "appid",
                "value": "{{appid}}"
            }
        ],
        "pathToData": "response.player_count",
        "headers": []
    },
    "metadata": [
        {
            "shape": [
                "number",
                {
                    "decimalPlaces": 0
                }
            ],
            "name": "value"
        },
        {
            "pattern": ".*"
        }
    ],
    "timeframes": false,
    "ui": [
        {
            "help": "App ID is a number in the link of the game's store page",
            "name": "appid",
            "label": "App ID",
            "placeholder": "730",
            "title": "App ID",
            "type": "text",
            "validation": {
                "required": true
            }
        }
    ]
}